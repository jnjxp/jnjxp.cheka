{
    "name": "jnjxp/cheka",
    "description": "Radar route ACL Authorization",
    "homepage":"https://github.com/jnjxp/jnjxp.cheka",
    "license": "MIT",
    "authors": [
        {
            "name": "jake johns",
            "email": "jake@jakejohns.net"
        }
    ],
    "require": {
        "psr/http-message": "^1.0",
        "radar/adr":"^1.0",
        "aura/router": "~3.0",
        "zendframework/zend-permissions-acl": "^2.6",
        "vperyod/auth-handler":"^0.2.0",
        "fusible/authrole":"dev-develop"
    },
    "require-dev":{
        "zendframework/zend-diactoros": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Jnjxp\\Cheka\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jnjxp\\Cheka\\":"tests/",
            "Aura\\Di\\": "vendor/aura/di/tests/"
        }
    },
    "minimum-stability": "dev"
}
